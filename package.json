{
  "name": "@oada/client",
  "version": "4.5.3",
  "description": "A lightweight client tool to interact with an OADA-compliant server",
  "repository": "https://github.com/OADA/client",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=14.18.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/event-iterator.js": {
      "node": "./dist/event-iterator.js",
      "default": "./dist/event-iterator-browser.js"
    },
    "./dist/fetch.js": {
      "node": "./dist/fetch.js",
      "default": "./dist/fetch-browser.js"
    },
    "./dist/file-type.js": {
      "node": "./dist/file-type.js",
      "default": "./dist/file-type-browser.js"
    }
  },
  "browser": {
    "./dist/event-iterator.js": "./dist/event-iterator-browser.js",
    "./dist/fetch.js": "cross-fetch",
    "events": false,
    "fetch-h2": false,
    "file-type": "./dist/file-type-browser.js",
    "resolve-alpn": false
  },
  "react-native": {
    "./dist/event-iterator.js": "./dist/event-iterator-browser.js",
    "./dist/fetch.js": "cross-fetch",
    "events": false,
    "fetch-h2": false,
    "file-type": "./dist/file-type-browser.js",
    "resolve-alpn": false
  },
  "files": [
    "lib/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "test": "$npm_execpath run build test && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads",
    "build": "tsc -b",
    "bundle": "webpack",
    "prepare": "$npm_execpath run build",
    "prestart": "$npm_execpath run build"
  },
  "ava": {
    "concurrency": 2,
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "lib/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "lib",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@oada/jobs": "^4.2.5",
    "@oada/types": "^3.5.2",
    "abort-controller": "^3.0.0",
    "buffer": "^6.0.3",
    "bufferutil": "^4.0.7",
    "cross-fetch": "^3.1.6",
    "debug": "^4.3.4",
    "deep-clone": "^4.0.0",
    "encoding": "^0.1.13",
    "event-iterator": "^2.0.0",
    "eventemitter3": "^5.0.1",
    "fetch-h2": "^3.0.2",
    "file-type": "^18.5.0",
    "isomorphic-timers-promises": "^1.0.1",
    "isomorphic-ws": "^5.0.0",
    "json-ptr": "^3.1.1",
    "jsonpath-plus": "^7.2.0",
    "media-type": "^0.3.1",
    "object-assign-deep": "^0.4.0",
    "p-queue": "^7.3.4",
    "reconnecting-websocket": "^4.4.0",
    "resolve-alpn": "^1.2.1",
    "serialize-error": "^11.0.0",
    "supports-color": "^9.3.1",
    "tslib": "^2.5.3",
    "utf-8-validate": "^6.0.3",
    "ws": "^8.13.0",
    "xksuid": "^0.0.3"
  },
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/debug": "^4.1.8",
    "@types/node": "^16.18.36",
    "@types/object-assign-deep": "^0.4.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@yarnpkg/sdks": "3.0.0-rc.45",
    "ava": "5.3.0",
    "axios": "^1.4.0",
    "c8": "^8.0.0",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-escompat": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "prettier": "^2.8.8",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@3.6.0",
  "volta": {
    "node": "16.16.0"
  },
  "resolutions": {
    "deep-clone@^4.0.0": "patch:deep-clone@npm%3A4.0.0#./.yarn/patches/deep-clone-npm-4.0.0-eee1a48c85.patch"
  }
}
